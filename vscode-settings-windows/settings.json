{
    "liveServer.settings.donotShowInfoMsg": true,
    "editor.fontSize": 18,
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "editor.renderControlCharacters": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "workbench.iconTheme": "material-icon-theme",
    "liveServer.settings.CustomBrowser": "chrome",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "liveServer.settings.donotVerifyTags": true,
    "editor.wordWrap": "on",
    "python.languageServer": "Pylance",
    "C_Cpp.updateChannel": "Insiders",
    "code-runner.clearPreviousOutput": true,
    "code-runner.runInTerminal": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.saveFileBeforeRun": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.mouseWheelZoom": true,
    "vsicons.dontShowNewVersionMessage": true,
    "tabnine.experimentalAutoImports": true,
    "files.exclude": {
        "*.bin": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.git": false,
        "**/.project": true,
        "**/.settings": true
    },
    "C_Cpp.errorSquiggles": "Enabled",
    "indentRainbow.colorOnWhiteSpaceOnly": true,
    "editor.linkedEditing": true,
    "git.confirmSync": false,
    "explorer.confirmDelete": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.sqlite3": "default"
    },
    "jupyter.alwaysTrustNotebooks": true,
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.startupEditor": "newUntitledFile",
    "files.associations": {
        "*.js": "javascript",
        "*.html": "html"
    },
    "editor.fontLigatures": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    }, // for emmet in react apps
    "gitlens.defaultDateFormat": null,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.scopes": [
        "document",
        "containers"
    ],
    "editor.cursorWidth": 5,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "cph.general.defaultLanguage": "cpp",
    "cph.general.menuChoices": "cpp java python c ",
    "explorer.confirmDragAndDrop": false,
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": true,
            "compare": true,
            "history": false,
            "remote": false
        },
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scm": {
            "authors": true
        },
        "scmGroupInline": {
            "stash": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": false,
            "stash": true
        }
    },
    "git.enableSmartCommit": true,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}Chetan's ${appName}",
    "editor.cursorBlinking": "expand",
    "editor.fastScrollSensitivity": 10,
    // ---------------------------------------------------------------------------------------------
    // Added by chetan if we remove above lines no harm will happen
    "terminal.integrated.fontFamily": "Roboto Mono for Powerline",
    "terminal.integrated.fontSize": 14,
    "workbench.colorCustomizations": {
        "terminal.ansiBlue": "#ADADAD",
        "terminal.ansiBlack": "#424242",
        "terminal.background": "#121212",
        "terminal.foreground": "#dedede"
    },
    // Added by chetan if we remove above lines no harm will happen
    // ---------------------------------------------------------------------------------------------
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.editor.untitled.hint": "hidden",
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
    }, /// for snippets quickSuggestions 
    "security.workspace.trust.untrustedFiles": "open",
    "[css]": {
        "editor.suggest.insertMode": "replace",
        "gitlens.codeLens.scopes": [
            "document"
        ]
    },
    "editor.inlineSuggest.enabled": true,
    "cph.language.python.SubmissionCompiler": "Python 3.7.2",
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": true,
        "markdown": true,
        "cpp": true,
        "python": true,
        "html": true,
        "dart": true
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[cpp]": {
        "editor.wordBasedSuggestions": false,
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true
    },
    "diffEditor.codeLens": true,
    "editor.formatOnType": true,
    "editor.quickSuggestionsDelay": 15,
    "workbench.preferredDarkColorTheme": "Monokai",
    "pdf-preview.default.sidebar": true,
    "editor.scrollbar.verticalScrollbarSize": 12,
    "screencastMode.fontSize": 30,
    "screencastMode.onlyKeyboardShortcuts": true,
    "screencastMode.verticalOffset": 6,
    "markdown-preview-enhanced.previewTheme": "github-light.css",
    "window.menuBarVisibility": "classic",
    "thunder-client.codeSnippetLanguage": "cs-httpclient",
    "go.toolsManagement.autoUpdate": true,
    "cSpell.userWords": [
        "ackermann",
        "acnt",
        "bcnt",
        "Cooldown",
        "cout",
        "curr",
        "getline",
        "inorder",
        "istringstream",
        "Leetcode",
        "memset",
        "nlogn",
        "nums",
        "Robo",
        "stdc",
        "stoi",
        "stringstream",
        "subarray",
        "subproblems",
        "substr",
        "tailwindcss",
        "vite",
        "Watchlist"
    ],
    "workbench.editor.autoLockGroups": {
        "mainThreadWebview-markdown.preview": true
    },
    "cSpell.ignoreWords": [
        "djangoflix",
        "xorr"
    ],
    "cph.general.ignoreSTDERROR": true,
    "cph.general.timeOut": 5000,
    "prettier.tabWidth": 4,
    "C_Cpp.clang_format_fallbackStyle": "WebKit",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "cph.general.autoShowJudge": false,
    "editor.formatOnSave": true,
    "dart.previewFlutterUiGuides": true,
    "workbench.colorTheme": "JellyFish",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "redhat.telemetry.enabled": false,
    "editor.fontFamily": "'Cascadia code',Consolas, 'Courier New', monospace",
    "workbench.productIconTheme": "fluent-icons",
    "window.zoomLevel": -1,
}